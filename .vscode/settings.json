{
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "eslint.packageManager": "yarn",
    "eslint.workingDirectories": [
      { "directory": "." },
    ],
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
      "javascript.suggest.completeFunctionCalls": true,
      "typescript.suggest.completeFunctionCalls": true,
  }